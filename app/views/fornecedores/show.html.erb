<div class="page-header">
  <%= link_to fornecedores_path, class: 'btn btn-default' do %>
    <span class="glyphicon glyphicon-list-alt"></span>
    Back
  <% end %>
  <%= link_to edit_fornecedore_path(@fornecedore), class: 'btn btn-primary' do %>
    <span class="glyphicon glyphicon-pencil"></span>
    Edit
  <% end %>
  <h1>Show fornecedore</h1>
</div>

<dl class="dl-horizontal">
  <dt>Nome:</dt>
  <dd><%= @fornecedore.nome %></dd>

  <dt>Tipo produtos:</dt>
  <dd><%= @fornecedore.tipo_produtos %></dd>

  <dt>Telefone:</dt>
  <dd><%= @fornecedore.telefone %></dd>

  <dt>Endereco:</dt>
  <dd><%= @fornecedore.endereco %></dd>

  <dt>Email:</dt>
  <dd><%= @fornecedore.email %></dd>

</dl>
